  if not game.Players.LocalPlayer:IsInGroup(10364016) then
    coroutine.resume(coroutine.create(function()
       wait(3)
       game.StarterGui:SetCore("SendNotification", {
        Title = 'Join our Roblox group';
        Text = 'youre still able to use the script but join the roblox group, paste it in your url enter and join it to support';
        Duration = 10;
       })
       setclipboard('https://www.roblox.com/groups/10364016/sukie#!/about')
    end))
  end
end)
loadstring(game:HttpGet("https://raw.githubusercontent.com/zvzi/scropts/main/private%20lol%20dont%20use%20this"))()
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Loot Legit Mode", "GrapeTheme")
local Tab = Window:NewTab("Main Scripts")
local Section = Tab:NewSection("Main Scripts")
Section:NewKeybind("Reset w/ Keybind", "Press X to reset!!", Enum.KeyCode.X, function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)
Section:NewToggle("AntiSlow", "shoot/attack without being slown down", function(state)
    if state then
    	if game.PlaceId == 4669040 then
local mt = getrawmetatable(game)
local backup
backup = hookfunction(mt.__newindex, newcclosure(function(self, key, value)
if key == "WalkSpeed" and value < 16 then
value = 16
end
return backup(self, key, value)
else
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)
Section:NewSlider("Speed", "Slide the slider down or up, indicating how fast player will be", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
